	.org $8000
	;first we have to bootstrap the brk vector code
	LDA #$A9 ;lda(#) instruction in lda
	STA $01
	LDA #$02 ;number to load into a at brk 
	STA $02
	LDA #$40 ;RTI instruction opcode	
	STA $03
	; bootstrapping done, normal program can run
	LDA #$01
	BRK 
	.byte $05
	LDY #69
	.byte $bb
	.org $FFFC
	.word $8000	
	.org $FFFE
	.word $1
